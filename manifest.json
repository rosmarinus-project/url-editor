{
  "manifest_version": 3,
  "name": "Url Editor",
  "description": "easy for editing url",
  "version": "1.0.1",
  "icons": {
    "16": "assets/logo_1x.png",
    "32": "assets/logo_2x.png",
    "48": "assets/logo_3x.png",
    "128": "assets/logo_4x.png"
  },
  "background": {
    "service_worker": "background/dist/es/index.js"
  },
  "action": {
    "default_popup": "popup/dist/index.html",
    "default_icon": "popup/dist/logo.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs"
  ]
}
